<section class="maison_commerce_maison_banner">
  <div class="maison_commerce_maison_banner__container">
    <div class="maison_commerce_maison_banner__content-wrapper">
      <!-- Left Column: Content -->
      <div class="maison_commerce_maison_banner__content">
        <div class="maison_commerce_maison_banner__text-content">
          <!-- Rating Badge -->
          {% if section.settings.show_rating %}
            <div class="maison_commerce_maison_banner__rating">
              <div class="maison_commerce_maison_banner__stars">
                {% for i in (1..5) %}
                  <svg xmlns="http://www.w3.org/2000/svg" class="maison_commerce_maison_banner__star" width="16" height="16" viewBox="0 0 16 16" fill="none">
                    <path d="M7.68323 1.52997C7.71245 1.47094 7.75758 1.42126 7.81353 1.38652C7.86949 1.35178 7.93404 1.33337 7.9999 1.33337C8.06576 1.33337 8.13031 1.35178 8.18626 1.38652C8.24222 1.42126 8.28735 1.47094 8.31656 1.52997L9.85656 4.6493C9.95802 4.85461 10.1078 5.03224 10.293 5.16694C10.4782 5.30164 10.6933 5.38938 10.9199 5.42264L14.3639 5.92664C14.4292 5.93609 14.4905 5.96362 14.5409 6.0061C14.5913 6.04859 14.6288 6.10434 14.6492 6.16704C14.6696 6.22975 14.6721 6.29691 14.6563 6.36093C14.6405 6.42495 14.6071 6.48327 14.5599 6.5293L12.0692 8.95464C11.905 9.1147 11.7821 9.31229 11.7111 9.53039C11.6402 9.74849 11.6233 9.98056 11.6619 10.2066L12.2499 13.6333C12.2614 13.6985 12.2544 13.7657 12.2296 13.8271C12.2048 13.8885 12.1632 13.9417 12.1096 13.9806C12.056 14.0196 11.9925 14.0426 11.9265 14.0472C11.8604 14.0518 11.7944 14.0378 11.7359 14.0066L8.65723 12.388C8.45438 12.2815 8.22868 12.2258 7.99956 12.2258C7.77044 12.2258 7.54475 12.2815 7.3419 12.388L4.2639 14.0066C4.20545 14.0376 4.1395 14.0515 4.07353 14.0468C4.00757 14.0421 3.94424 14.019 3.89076 13.9801C3.83728 13.9412 3.79579 13.8881 3.771 13.8268C3.74622 13.7655 3.73914 13.6984 3.75056 13.6333L4.3379 10.2073C4.37669 9.98112 4.35989 9.7489 4.28892 9.53067C4.21796 9.31243 4.09497 9.11474 3.93056 8.95464L1.4399 6.52997C1.39229 6.48399 1.35856 6.42557 1.34254 6.36135C1.32652 6.29714 1.32886 6.22971 1.34928 6.16676C1.36971 6.10381 1.40741 6.04786 1.45808 6.00529C1.50876 5.96272 1.57037 5.93524 1.6359 5.92597L5.07923 5.42264C5.30607 5.38964 5.52149 5.30201 5.70695 5.16729C5.89242 5.03258 6.04237 4.85482 6.1439 4.6493L7.68323 1.52997Z" fill="{{ section.settings.star_color }}" stroke="{{ section.settings.star_color }}" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg>
                {% endfor %}
              </div>
              <p class="maison_commerce_maison_banner__rating-text">
                Rated <strong>{{ section.settings.rating_value }}</strong> based on <strong>{{ section.settings.review_count }}</strong> reviews.
              </p>
            </div>
          {% endif %}

          <!-- Heading -->
          {% if section.settings.heading != blank %}
            <h1 class="maison_commerce_maison_banner__heading">
              {{ section.settings.heading }}
            </h1>
          {% endif %}

          <!-- Body Text -->
          {% if section.settings.body_text != blank %}
            <p class="maison_commerce_maison_banner__body-text">
              {{ section.settings.body_text }}
            </p>
          {% endif %}
        </div>

        <!-- Feature Badges -->
        {% if section.blocks.size > 0 %}
          <div class="maison_commerce_maison_banner__features">
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'feature_badge' %}
                  <div class="maison_commerce_maison_banner__feature-badge" {{ block.shopify_attributes }}>
                    <svg xmlns="http://www.w3.org/2000/svg" class="maison_commerce_maison_banner__check-icon" width="18" height="18" viewBox="0 0 18 18" fill="none">
                        <path d="M8.8125 0C3.94549 0 0 3.94549 0 8.8125C0 13.6795 3.94549 17.625 8.8125 17.625C13.6795 17.625 17.625 13.6795 17.625 8.8125C17.625 3.94549 13.6795 0 8.8125 0Z" fill="#CCCCE8"/>
                        <path d="M12.1454 6.49451L7.2289 11.411L4.99414 9.17622" stroke="{{ block.settings.checkmark_color }}" stroke-width="1.45455" stroke-linecap="square" stroke-linejoin="round"/>
                    </svg>
                    <span class="maison_commerce_maison_banner__feature-text">{{ block.settings.feature_text }}</span>
                  </div>
              {% endcase %}
            {% endfor %}
          </div>
        {% endif %}

        <!-- CTA Buttons -->
        <div class="maison_commerce_maison_banner__buttons">
          {% if section.settings.primary_button_url != blank %}
            <a href="{{ section.settings.primary_button_url }}" class="maison_commerce_maison_banner__button maison_commerce_maison_banner__button--primary">
              <span>{{ section.settings.primary_button_text }}</span>
              <svg xmlns="http://www.w3.org/2000/svg" class="maison_commerce_maison_banner__arrow-icon" width="18" height="18" viewBox="0 0 18 18" fill="none">
                <g clip-path="url(#clip0_10248_6079)">
                    <path d="M17.7825 8.46752L14.1975 4.88252C14.092 4.77845 13.9581 4.70796 13.8126 4.67993C13.6671 4.6519 13.5166 4.66759 13.38 4.72502C13.243 4.78129 13.1258 4.87684 13.043 4.99963C12.9603 5.12242 12.9157 5.26695 12.915 5.41502V7.66502C12.915 7.71475 12.8952 7.76244 12.8601 7.7976C12.8249 7.83277 12.7772 7.85252 12.7275 7.85252H1.125C0.826631 7.85252 0.540483 7.97105 0.329505 8.18203C0.118526 8.39301 0 8.67915 0 8.97752C0 9.27589 0.118526 9.56204 0.329505 9.77302C0.540483 9.984 0.826631 10.1025 1.125 10.1025H12.75C12.7997 10.1025 12.8474 10.1223 12.8826 10.1574C12.9177 10.1926 12.9375 10.2403 12.9375 10.29V12.54C12.9375 12.7389 13.0165 12.9297 13.1572 13.0704C13.2978 13.211 13.4886 13.29 13.6875 13.29C13.7867 13.2896 13.8849 13.2695 13.9763 13.2309C14.0677 13.1923 14.1505 13.1359 14.22 13.065L17.805 9.48752C17.9239 9.35211 17.9927 9.18004 18 9.00002C18.0006 8.90132 17.9817 8.80347 17.9443 8.71209C17.907 8.62071 17.852 8.5376 17.7825 8.46752Z" fill="currentColor"/>
                </g>
                <defs>
                    <clipPath id="clip0_10248_6079">
                    <rect width="18" height="18" fill="white"/>
                    </clipPath>
                </defs>
              </svg>
            </a>
          {% endif %}

          {% if section.settings.secondary_button_url != blank %}
            <a href="{{ section.settings.secondary_button_url }}" class="maison_commerce_maison_banner__button maison_commerce_maison_banner__button--secondary">
              <span>{{ section.settings.secondary_button_text }}</span>
            </a>
          {% endif %}
        </div>
      </div>

      <!-- Right Column: Image -->
      {% if section.settings.banner_image != blank %}
        <div class="maison_commerce_maison_banner__image-wrapper">
          <img 
            src="{{ section.settings.banner_image | image_url: width: 1322 }}" 
            alt="{{ section.settings.banner_image.alt | escape }}"
            class="maison_commerce_maison_banner__image"
            width="{{ section.settings.banner_image.width }}"
            height="{{ section.settings.banner_image.height }}"
            loading="lazy"
          />
        </div>
      {% endif %}
    </div>
  </div>
</section>

<style>
  #shopify-section-{{ section.id }}{
    padding: 0;
  }
  /* Section Container */
  .maison_commerce_maison_banner {
    background-color: {{ section.settings.section_bg_color }};
    padding-top: {{ section.settings.padding_top_desktop }}px;
    padding-bottom: {{ section.settings.padding_bottom_desktop }}px;
    width: 100%;
    max-width: 1440px;
    margin: 0 auto;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner {
      padding-top: {{ section.settings.padding_top_mobile }}px;
      padding-bottom: {{ section.settings.padding_bottom_mobile }}px;
    }
  }

  /* Container */
  .maison_commerce_maison_banner__container {
    max-width: 100%;
    margin: 0 auto;
    padding-left: {{ section.settings.padding_left_desktop }}px;
    padding-right: {{ section.settings.padding_right_desktop }}px;
  }

  /* Content Wrapper - Two Column Layout */
  .maison_commerce_maison_banner__content-wrapper {
    display: flex;
    align-items: center;
    gap: 80px;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__content-wrapper {
      flex-direction: column-reverse;
      gap: 0;
    }
  }

  /* Left Column: Content */
  .maison_commerce_maison_banner__content {
    width: 100%;
    max-width: 619px;
    display: flex;
    flex-direction: column;
    gap: 24px;
    padding: 18px 0;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__content {
      flex: 1;
      width: 100%;
      padding: 20px;
      gap: 18px;
    }
  }

  /* Text Content */
  .maison_commerce_maison_banner__text-content {
    display: flex;
    flex-direction: column;
    gap: 18px;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__text-content {
      gap: 8px;
      align-items: center;
    }
  }

  /* Rating Badge */
  .maison_commerce_maison_banner__rating {
    display: flex;
    gap: 8px;
    align-items: center;
    justify-content: flex-start;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__rating {
      justify-content: center;
    }
  }

  .maison_commerce_maison_banner__stars {
    display: flex;
    gap: 0;
    align-items: center;
  }

  .maison_commerce_maison_banner__star {
    width: 16px;
    height: 16px;
    flex-shrink: 0;
  }

  .maison_commerce_maison_banner__rating-text {
    font-family: 'Neue Haas Grotesk Display Pro', sans-serif;
    font-weight: 300;
    font-size: {{ section.settings.rating_font_size_desktop }}px;
    line-height: 1.5;
    color: {{ section.settings.rating_text_color }};
    margin: 0;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__rating-text {
      font-size: {{ section.settings.rating_font_size_mobile }}px;
    }
  }

  .maison_commerce_maison_banner__rating-text strong {
    font-weight: 500;
  }

  /* Heading */
  .maison_commerce_maison_banner__heading {
    font-family: 'Vanilla Ravioli', serif;
    font-size: {{ section.settings.heading_font_size_desktop }}px;
    font-weight: 400;
    line-height: {{ section.settings.heading_line_height_desktop }};
    color: {{ section.settings.heading_color }};
    margin: 0;
    text-align: left;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__heading {
      font-size: {{ section.settings.heading_font_size_mobile }}px;
      line-height: {{ section.settings.heading_line_height_mobile }};
      text-align: center;
    }
  }

  /* Body Text */
  .maison_commerce_maison_banner__body-text {
    font-family: 'Neue Haas Grotesk Display Pro', sans-serif;
    font-size: {{ section.settings.body_font_size_desktop }}px;
    font-weight: 500;
    line-height: 1.5;
    color: {{ section.settings.body_color }};
    margin: 0;
    text-align: left;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__body-text {
      font-family: 'Gotham', sans-serif;
      font-weight: 400;
      font-size: {{ section.settings.body_font_size_mobile }}px;
      text-align: center;
    }
  }

  /* Features Container */
  .maison_commerce_maison_banner__features {
    display: flex;
    gap: 12px;
    flex-wrap: wrap;
    align-items: center;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__features {
      justify-content: space-between;
      gap: 8px;
    }
  }

  /* Feature Badge */
  .maison_commerce_maison_banner__feature-badge {
    display: flex;
    gap: 8px;
    align-items: center;
    justify-content: center;
    padding: 4px 15px;
    border-radius: 8px;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__feature-badge {
      padding: 4px 0;
    }
  }

  .maison_commerce_maison_banner__check-icon {
    width: 18px;
    height: 18px;
    flex-shrink: 0;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__check-icon {
      width: 16px;
      height: 16px;
    }
  }

  .maison_commerce_maison_banner__feature-text {
    font-family: 'Gotham', sans-serif;
    font-size: 12px;
    font-weight: 400;
    line-height: 1.5;
    color: {{ section.settings.feature_text_color }};
    white-space: nowrap;
  }

  /* Buttons Container */
  .maison_commerce_maison_banner__buttons {
    display: flex;
    gap: 24px;
    align-items: flex-start;
    flex-wrap: wrap;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__buttons {
      flex-direction: column;
      gap: 18px;
      width: 100%;
    }
  }

  /* Button Base Styles */
  .maison_commerce_maison_banner__button {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    padding: 16px 32px;
    border-radius: 12px;
    text-decoration: none;
    transition: opacity 0.3s ease;
    height: 56px;
    min-width: fit-content;
    box-sizing: border-box;
  }

  .maison_commerce_maison_banner__button:hover {
    opacity: 0.9;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__button {
      width: 100%;
      padding: 16px 32px;
    }
  }

  /* Primary Button */
  .maison_commerce_maison_banner__button--primary {
    background-color: {{ section.settings.primary_button_bg_color }};
    color: {{ section.settings.button_text_color }};
    flex: 1;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__button--primary {
      flex: none;
    }
  }

  .maison_commerce_maison_banner__button--primary span {
    font-family: 'Neue Haas Grotesk Display Pro', sans-serif;
    font-size: {{ section.settings.primary_button_font_size_desktop }}px;
    font-weight: 500;
    line-height: 1.5;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__button--primary span {
      font-family: 'Gotham', sans-serif;
      font-size: {{ section.settings.primary_button_font_size_mobile }}px;
      font-weight: 500;
    }
  }

  .maison_commerce_maison_banner__arrow-icon {
    width: 18px;
    height: 18px;
    flex-shrink: 0;
  }

  /* Secondary Button */
  .maison_commerce_maison_banner__button--secondary {
    background-color: {{ section.settings.secondary_button_bg_color }};
    color: {{ section.settings.button_text_color }};
    flex: 1;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__button--secondary {
      flex: none;
    }
  }

  .maison_commerce_maison_banner__button--secondary span {
    font-family: 'Neue Haas Grotesk Display Pro', sans-serif;
    font-size: {{ section.settings.secondary_button_font_size_desktop }}px;
    font-weight: 500;
    line-height: 1.5;
  }

  /* Image Wrapper */
  .maison_commerce_maison_banner__image-wrapper {
    width: 100%;
    max-width: 661px;
    height: 100%;
    display: flex;
    position: relative;
    overflow: hidden;
    border-radius: 32px 0 0 32px;
    max-height: 598px;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__image-wrapper {
      flex: none;
      width: 100%;
      max-width: 100%;
      border-radius: 0;
    }
  }

  .maison_commerce_maison_banner__image {
    width: 100%;
    height: auto;
    object-fit: cover;
    object-position: center;
    aspect-ratio: 1 / 0.9047;
  }

  @media (max-width: 768px) {
    .maison_commerce_maison_banner__image {
      aspect-ratio: 1 / 0.65867;
    }
  }

</style>

